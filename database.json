{
  "development": {
    "driver": "sqlite3",
    "filename": "~/dev.db"
  },
  "stage": {
    "driver": "sqlite3",
    "filename": ":memory:"
  },
  "homolog": {
    "driver": "sqlite3",
    "filename": ":memory:"
  },
  "production": {
    "driver": "pg",
    "user": {"ENV": "DB_USERNAME"},
    "password": {"ENV": "DB_PASSWORD"},
    "host": {"ENV": "DB_HOST"},
    "database": {"ENV": "DB_DATABASE"},
    "schema": {"ENV": "DB_SCHEMA"}
  },
  "defaultEnv": "development"
}
